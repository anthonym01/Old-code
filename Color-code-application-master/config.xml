<?xml version='1.8.2' encoding='utf-8'?>
<widget id="com.ANTHONYM.ColorCodes" version="1.5.3" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Color Codes</name>
    <description>
        Hexidecimal, RGB and HSL color Codes
    </description>
    <author email="samuelmatheson15@gmail.com" href="mailto:samuelmatheson15@gmail.com">
        Samuel A. Matheson
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:18765744891" />
    <allow-intent href="sms:18765744801" />
    <allow-intent href="mailto:samuelmatheson15@gmail.com" />
    <allow-intent href="geo:*" />
    <icon src="res/icon.png"/>
    <platform name="android">
        <allow-intent href="market:*" />
        <icon src="res/icon/icon.png" />
        <preference name="android-minSdkVersion" value="19" />
        <preference name="android-targetSdkVersion" value="27" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
